{
  "name": "@wishlist/backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Express backend API for wishlist application",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:smoke": "vitest run tests/smoke.test.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:browser": "vitest run --config vitest.e2e.browser.config.ts",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@wishlist/shared": "*",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.8",
    "helmet": "^8.1.0",
    "inversify": "^7.10.7",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.15.0",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.19.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "prisma": "^6.19.1",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
