---
import Layout from '../layouts/Layout.astro';
import { TermsOfServicePageWrapper } from '../components/TermsOfServicePageWrapper';

const appUrl = import.meta.env.PUBLIC_APP_URL;
const loginUrl = `${appUrl}/login`;
---

<Layout title="Terms of Service - Wish Master">
  <TermsOfServicePageWrapper client:load loginUrl={loginUrl} />
</Layout>
